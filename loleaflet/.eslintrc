{
  "extends": "eslint:recommended",
  "rules": {
    "camelcase": 2,
    "quotes": [2, "single"],
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "space-in-parens": 2,
    "space-before-blocks": 2,
    "keyword-spacing": 2,
    "no-lonely-if": 2,
    "comma-style": 2,
    "indent": [2, "tab", {"VariableDeclarator": 0}],
    "no-underscore-dangle": 0,
    "no-constant-condition": 0,
    "no-multi-spaces": 0,
    "strict": 0,
    "key-spacing": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-control-regex": 0,
    "no-useless-escape": 0,
    "semi": 2
  },
  "globals": {
    "L": true,
    "module": false,
    "define": false
  },
  "env": {
    "browser": true
  },
}
