msgstr "Bir ekran sayfasını sağa taşır."

#: html/cool-help.html%2Bdiv.div.table.tr.td:146-80
msgid "Alt</kbd><span class=\"kbd--plus\">+</span><kbd>Page Down"
msgstr "Alt</kbd><span class=\"kbd--plus\">+</span><kbd>Page Down"

