#: html/cool-help.html%2Bdiv.div.table.tr.td:108-18
msgid "Alt</kbd><span class=\"kbd--plus\">+</span><kbd>Insert"
msgstr "Alt</kbd><span class=\"kbd--plus\">+</span><kbd>Inser"

#: html/cool-help.html%2Bdiv.div.table.tr.td:108-109
msgid ""
