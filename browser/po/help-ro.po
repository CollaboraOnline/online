msgstr "<kbd>Shift</kbd><span class=\"kbd--plus\">+</span><kbd>Enter</kbd>"

#: html/cool-help.html%2Bdiv.div.div.table.tr.td:123-38
msgid "Manual page break"
msgstr "Întrerupere manuală de pagină"

